{
  "name": "api-errands",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only ./src/index.ts",
    "postinstall": "tsc",
    "start": "node ./build/index.js",
    "build": "npx tsc",
    "typeorm:generate": "npm run typeorm migration:generate -- -d ./src/main/config/database.config.ts",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/main/config/database.config.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:create": "typeorm migration:create",
    "typeorm:revert": "npm run typeorm migration:revert -- -d ./src/main/config/database.config.ts",
    "migration:create": "typeorm migration:create src/database/migrations/AlterTable",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/config/database.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/database/config/database.config.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ErrandEntity -d src/database/config/database.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/database/config/database.config.ts",
    "test": "cross-env DB_ENV=test jest --silent --noStackTrace --runInBand",
    "test:verbose": "cross-env DB_ENV=test jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "cross-env DB_ENV=test jest --runInBand --coverage",
    "test:watch": "cross-env DB_ENV=test jest --silent --noStackTrace --runInBand --watchAll",
    "typeorm:run:test": "cross-env DB_ENV=test npm run typeorm migration:run -- -d ./src/main/config/database.config.ts",
    "typeorm:generate:test": "cross-env DB_ENV=test npm run typeorm:generate -- tests/app/shared/database/migrations/TestMigration",
    "test:setup": "rimraf db.sqlite3 && rimraf ./tests/app/shared/database/migrations && npm run typeorm:generate:test && npm run typeorm:run:test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "jest": "^29.6.3",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  }
}
